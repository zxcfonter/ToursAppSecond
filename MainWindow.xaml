<Window x:Class="ToursAppSecond.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToursAppSecond"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="42*"/>
            <RowDefinition Height="332*"/>
            <RowDefinition Height="45*"/>
        </Grid.RowDefinitions>
        <Image Source="Resources/лого.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Header" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Grid Background="#bae3e8" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="#445c93"></Grid>
        <Frame Grid.Row="1"></Frame>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered_1" Grid.Row="1" Name="MainFrame"></Frame>
        <Button Content="Back" Name="BtnBack"  HorizontalAlignment="Right"  Click="BtnBack_Click"></Button>

    </Grid>
</Window>
